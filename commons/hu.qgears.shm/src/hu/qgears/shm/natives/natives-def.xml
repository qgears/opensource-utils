<?xml version="1.0" encoding="UTF-8"?>
<natives-def>
	<platform name="Linux">
		<platform arch="(amd|x86_)64">
			<platform distroId="ubuntu" distroVersion="14.04">
				<lib id="libshm" path="libshm-ubuntu-14.04-x86_64.so" installPath="/opt/qgears/libshm-ubuntu-14.04-x86_64.so"/>
			</platform>
			<platform distroId="ubuntu" distroVersion="18.04">
				<lib id="libshm" path="libshm-ubuntu-18.04-x86_64.so" installPath="/opt/qgears/libshm-ubuntu-18.04-x86_64.so"/>
			</platform>
			<platform distroId="debian" distroVersion="9">
				<lib id="libshm" path="libshm-debian-9-x86_64.so" installPath="/opt/qgears/libshm-debian-9-x86_64.so"/>
			</platform>
			<platform distroId="debian" distroVersion="10">
				<lib id="libshm" path="libshm-debian-10-x86_64.so" installPath="/opt/qgears/libshm-debian-10-x86_64.so"/>
			</platform>
			<lib id="libshm" path="libshm-x86_64.so" installPath="/opt/qgears/libshm-x86_64.so"/>
		</platform>
		<platform arch="x86|i[3-6]86">
			<platform distroId="debian" distroVersion="9">
				<lib id="libshm" path="libshm-debian-9-i686.so" installPath="/opt/qgears/libshm-debian-9-i686.so"/>
			</platform>
			<platform distroId="debian" distroVersion="10">
				<lib id="libshm" path="libshm-debian-10-i686.so" installPath="/opt/qgears/libshm-debian-10-i686.so"/>
			</platform>
			<platform distroId="ubuntu" distroVersion="14.04">
				<lib id="libshm" path="libshm-ubuntu-14.04-i686.so" installPath="/opt/qgears/libshm-ubuntu-14.04-i686.so"/>
			</platform>
			<lib id="libshm" path="libshm-i686.so" installPath="/opt/qgears/libshm-i686.so"/>
		</platform>
	</platform>
	<platform name="Windows .+">
		<platform arch="x86|i[3-6]86">
			<lib path="shm.dll" />
		</platform>
		<platform arch="(amd|x86_)64">
			<lib path="shm.dll" />
		</platform>
	</platform>
</natives-def>
