<?xml version="1.0" encoding="UTF-8"?>
<natives-def>
	<platform name="Linux">
		<platform arch="(amd|x86_)64">
			<lib path="libqglut64.so"
			installPath="/opt/qgears/libqglut64.so" />
		</platform>
		<platform arch="(i386)">
			<lib path="libqglut.so" 
				installPath="/opt/qgears/libqglut.so" />
		</platform>
	</platform>
	<platform name=".*(W|w)in.*">
		<platform arch="(amd|x86_)64">
			<lib path="libqglut64.dll"/>
		</platform>
		<platform arch="(i386)">
			<lib path="libqglut32.dll"/>
		</platform>
	</platform>
</natives-def>
