<?xml version="1.0" encoding="UTF-8"?>
<natives-def>
	<platform name=".*(W|w)in.*">
		<platform arch="x86|i[3-6]86">
			<lib path="native/windows/jinput-dx8.dll"/>
			<lib path="native/windows/jinput-raw.dll"/>
		</platform>
		<platform arch="(amd|x86_)64">
			<lib path="native/windows/jinput-dx8_64.dll"/>
			<lib path="native/windows/jinput-raw_64.dll"/>
		</platform>
	</platform>
	<platform name="Linux">
		<platform arch="(amd|x86_)64">
			<lib path="native/linux/libjinput-linux64.so" installPath="/opt/qgears/native_linux_libjinput-linux64.so" />
		</platform>
		<platform arch="x86|i[3-6]86">
			<lib path="native/linux/libjinput-linux.so" installPath="/opt/qgears/native_linux_libjinput-linux.so" />
		</platform>
	</platform>
</natives-def>