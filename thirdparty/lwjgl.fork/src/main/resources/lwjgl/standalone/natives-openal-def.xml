<?xml version="1.0" encoding="UTF-8"?>
<natives-def>
	<platform name=".*(W|w)in.*">
		<platform arch="x86|i[3-6]86">
			<lib path="native/windows/OpenAL32.dll"/>
		</platform>
		<platform arch="(amd|x86_)64">
			<lib path="native/windows/OpenAL64.dll"/>
		</platform>
	</platform>
	<platform name="Linux">
		<platform arch="(amd|x86_)64">
			<lib path="native/linux/libopenal64.so" installPath="/opt/qgears/native_linux_libopenal64.so" />
		</platform>
		<platform arch="x86|i[3-6]86">
			<lib path="native/linux/libopenal.so" installPath="/opt/qgears/native_linux_libopenal.so" />
		</platform>
	</platform>
</natives-def>