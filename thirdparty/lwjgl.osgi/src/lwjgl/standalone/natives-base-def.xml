			<lib path="native/windows/lwjgl.dll"/>
		</platform>
		<platform arch="(amd|x86_)64">
			<lib path="native/windows/lwjgl64.dll"/>
		</platform>
	</platform>
	<platform name="Linux">
		<platform arch="(amd|x86_)64">
			<lib path="native/linux/liblwjgl64.so" installPath="/opt/qgears/liblwjgl64.so" />
		</platform>
		<platform arch="x86|i[3-6]86">
			<lib path="native/linux/liblwjgl.so" installPath="/opt/qgears/liblwjgl.so" />
		</platform>
	</platform>
</natives-def>